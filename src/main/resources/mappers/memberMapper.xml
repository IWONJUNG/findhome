<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.itwillbs.mapper.MemberMapper">

	<insert id="insertMember">
		insert into member_seller (id, password, phone_number, is_confirm) values(#{id},#{password},#{phone_number},#{is_confirm})
	</insert>
	
	<select id="getMember" resultType="com.itwillbs.domain.MemberBean">
		select * from member_seller where id = #{id}
	</select>
  
</mapper>






